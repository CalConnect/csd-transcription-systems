# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_size = 2
indent_style = space

[{Makefile,*.make,*.sh,scripts/*}]
indent_size = 8
indent_style = tab

[codes.csv]
charset = utf-8-bom
